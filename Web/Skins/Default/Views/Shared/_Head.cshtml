@AspDotNetStorefront.Optimization.AdnsfBundler.RenderStyleBundle(
    bundlePath: Url.SkinUrl("css/bundled"),
    filePaths: new[]
    {
        Url.AppRelativeSkinUrl("css/bootstrap.css"),
        Url.AppRelativeSkinUrl("css/font-awesome.css"),
        Url.AppRelativeSkinUrl("css/base.css"),
        Url.AppRelativeSkinUrl("css/style.css"),
        Url.AppRelativeSkinUrl("Css/icomoon/style.css"),
        Url.AppRelativeSkinUrl("css/guidednavigation.css"),
        Url.AppRelativeSkinUrl("css/jquery-ui.css"),
        Url.AppRelativeSkinUrl("css/jquery-ui.structure.css"),
        Url.AppRelativeSkinUrl("css/main.css"),
        Url.AppRelativeSkinUrl("css/blue.css")

    }
)

